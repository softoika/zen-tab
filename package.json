{
  "name": "zen-tab",
  "version": "0.0.0",
  "description": "The tab management tool for Chrome",
  "author": "softoika <zwgntz@gmail.com>",
  "license": "MIT",
  "scripts": {
    "build": "yarn clean && TAILWIND_MODE=build NODE_ENV=production webpack",
    "build:dev": "yarn clean && TAILWIND_MODE=build NODE_ENV=development webpack",
    "build:watch": "yarn clean && TAILWIND_MODE=watch NODE_ENV=development webpack --watch",
    "test": "jest",
    "test:debug": "jest --silent false",
    "lint": "eslint src/ && prettier --check src/",
    "format": "prettier --write src/",
    "clean": "rm -rf dist/"
  },
  "dependencies": {
    "@tailwindcss/forms": "0.3.3",
    "autoprefixer": "10.2.6",
    "dayjs": "1.10.5",
    "postcss": "8.3.5",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "tailwindcss": "2.2.2"
  },
  "devDependencies": {
    "@testing-library/react-hooks": "6.0.0",
    "@types/chrome": "0.0.145",
    "@types/jest": "26.0.23",
    "@types/react": "17.0.11",
    "@types/react-dom": "17.0.8",
    "@typescript-eslint/eslint-plugin": "4.27.0",
    "@typescript-eslint/parser": "4.27.0",
    "copy-webpack-plugin": "9.0.0",
    "css-loader": "5.2.6",
    "eslint": "7.29.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-jsx-a11y": "6.4.1",
    "eslint-plugin-react": "7.24.0",
    "eslint-plugin-react-hooks": "4.2.0",
    "jest": "26.6.3",
    "jest-chrome": "0.7.1",
    "postcss-loader": "6.1.0",
    "prettier": "2.3.1",
    "react-test-renderer": "17.0.2",
    "style-loader": "2.0.0",
    "terser-webpack-plugin": "5.1.3",
    "ts-jest": "26.5.6",
    "ts-loader": "9.2.3",
    "tsconfig-paths-webpack-plugin": "3.5.1",
    "typescript": "4.3.4",
    "webextension-polyfill": "0.8.0",
    "webextension-polyfill-ts": "0.25.0",
    "webpack": "5.39.1",
    "webpack-cli": "4.7.2"
  }
}
